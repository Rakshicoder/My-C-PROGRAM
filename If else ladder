include <stdio.h>

 main() {
    int marks;
    printf("enter the marks");
    scanf("%d",&marks);
    if(marks<40)
    printf("fail");
    else if (marks<50)
    printf("pass")
    else if(marks<60)
    printf("second class")
    else if(marks<70)
    printf("first class")
    else if(marks<90)
    printf("distinction");
    else
    printf("out standing");



}
